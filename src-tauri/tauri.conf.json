{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pdf-generator-prototype",
  "version": "0.0.0",
  "identifier": "me.matthewvance.pdf-generator-prototype",
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "TAURI=1 pnpm dev",
    "beforeBuildCommand": "pnpm build"
  },
  "app": {
    "windows": [
      {
        "title": "PDF Generator Prototype",
        "width": 1320,
        "height": 1064,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "windows": {
      "nsis": {
        "installerIcon": "icons/icon.png"
      }
    }
  }
}
